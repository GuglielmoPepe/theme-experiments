{
	"version": 1,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank-page",
			"title": "Blank Page"
		}
	],
	"settings": {
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		},
		"color": {
			"duotone": [

			],
			"gradients": [

			],
			"palette": [
				{
					"slug": "cornsilk",
					"color": "cornsilk",
					"name": "Corn Silk"
				},
				{
					"slug": "orangered",
					"color": "orangered",
					"name": "Orange Red"
				},
				{
					"slug": "cornflowerblue",
					"color": "cornflowerblue",
					"name": "Cornflower Blue"
				},
				{
					"slug": "secondary",
					"color": "#fff",
					"name": "Off White"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1020px"
		},
		"blocks": {
			"core/heading": {
				"color": {
					"palette": [
						{
							"slug": "purple",
							"color": "purple",
							"name": "Purple"
						}
					]
				},
				"typography": {
					"customFontSize": false
				}
			},
			"core/group": {
				"elements": {
					"h2": {
						"color": "var(--wp--preset--color--cornflowerblue)"
					}
				}
			}
		},
		"typography": {
			"customFontSize": true,
			"customFontStyle": true,
			"customFontWeight": true,
			"customTextDecoration": true,
			"customTextTransformations": true,
			"dropCap": true,
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1.2rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "2.2rem",
					"name": "Large"
				}
			],
			"fontFamilies": [
                {
                    "fontFamily": "Times New Roman, serif",
                    "slug": "serif",
                    "name": "Serif"
                },
                {
                    "fontFamily": "Helvetica Neue, Arial, sans-serif",
                    "slug": "sans",
                    "name": "Sans serif"
                }
            ]
		},
		"spacing": {
			"customPadding": true,
			"units": [ "px", "em", "rem", "vh", "vw" ]
		},
		"custom": {

		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--cornsilk)",
			"text": "var(--wp--preset--color--orangered)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--cornflowerblue)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--serif)"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": 1.4,
					"dropCap": true
				}
			},
			"core/post-date": {
				"spacing": {
					"padding": {
						"right": "1rem",
						"left": "1rem"
					}
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"right": "1rem",
						"left": "1rem"
					}
				}
			},
			"core/post-title": {
				"spacing": {
					"padding": {
						"right": "1rem",
						"left": "1rem"
					}
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"background": "var(--wp--preset--color--blue)"
						},
						"border": {
							"style": "dotted",
							"width": "1px"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "1rem",
						"right": "1rem"
					}
				}
			}
		}
	}
}